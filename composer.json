{
  "name": "brezgalov/pt-zernovozam-api-client",
  "description": "api client for zernovozam",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Oleg Brezgalov",
      "email": "oleg.brezgalov@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.2.0",
    "ext-mbstring": "*",
    "yiisoft/yii2": ">=2.0.6",
    "brezgalov/yii2-base-api-client": "dev-master"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5"
  },
  "scripts": {
    "test": "./vendor/bin/phpunit test",
    "test-coverage": "./vendor/bin/phpunit test --coverage-html coverage"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ],
  "autoload": {
    "psr-4": {
      "Brezgalov\\ZernovozamApiClient\\": "src/",
      "Brezgalov\\ZernovozamApiClient\\UnitTests\\": "test/UnitTests/"
    }
  },
  "config": {
    "allow-plugins": {
      "yiisoft/yii2-composer": true
    }
  }
}
